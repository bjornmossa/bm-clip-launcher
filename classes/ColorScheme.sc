ColorScheme {
  classvar <background;
  classvar <enabled;
  classvar <enabled2;
  classvar <disabled;
  classvar <disabled2;

  *initClass {
	background = Color.fromHexString("#313135");
	enabled = Color.fromHexString("#c4897a");
	enabled2 = Color.fromHexString("#83a3c0");
	disabled = Color.fromHexString("#3d3d44");
	disabled2 = Color.fromHexString("#3d3d44");
  }
}